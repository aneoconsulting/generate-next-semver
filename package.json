{
  "name": "generate-next-semver",
  "version": "0.0.0",
  "private": true,
  "description": "Generate a new version using latest commit messages and following the semantic versioning",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barbapapazes/generate-next-semver.git"
  },
  "keywords": [
    "actions",
    "semver",
    "version"
  ],
  "main": "lib/main.js",
  "scripts": {
    "action": "jiti ./src/main.ts",
    "build": "tsc",
    "package": "ncc build --source-map --license licences.txt",
    "lint": "eslint --cache . --ext .ts",
    "format": "eslint --fix . --ext .ts",
    "all": "nr lint && nr build && nr package"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "changelogen": "^0.4.1",
    "semver": "^7.3.8",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/ni": "^0.20.0",
    "@types/node": "^18.14.2",
    "@types/semver": "^7.3.13",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.35.0",
    "jiti": "^1.17.1",
    "typescript": "^4.9.5"
  }
}
